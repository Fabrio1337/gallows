<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER"
      spacing="20"
      style="-fx-background-color: white;"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.gallows.GallowsThemes"
      prefHeight="300.0" prefWidth="200.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="theme"/>
    <Button text="Страны" onAction="#countrys"/>
    <Button text="Города" onAction="#citys"/>
    <Button text="Еда" onAction="#foods"/>
    <Button text="Животные" onAction="#animals"/>
    <Button text="Хобби" onAction="#hobbies"/>
    <Button text="Случайная" onAction="#random"/>
</VBox>
